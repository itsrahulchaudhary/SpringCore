<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
                
          <bean id="mathCheatObjectValue"  class="com.springcore.springDI.bySetter.objectType.loopsecoupling.MathCheat"></bean>    
          
          <bean id="scienceCheatObjectValue"  class="com.springcore.springDI.bySetter.objectType.loopsecoupling.ScienceCheat"></bean>      
          
          <bean id="javaCheatObjectValue"  class="com.springcore.springDI.bySetter.objectType.loopsecoupling.JavaCheat"></bean>          
                
          <bean id="student" class="com.springcore.springDI.bySetter.objectType.loopsecoupling.Student">
            <property name="cheat" ref="javaCheatObjectValue" />
          </bean>  
            
            
           
                
</beans>